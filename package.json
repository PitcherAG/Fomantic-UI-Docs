{
  "name": "fomantic-ui-docs",
  "title": "Fomantic UI Docs",
  "description": "Fomantic empowers designers and developers by creating a shared vocabulary for UI.",
  "homepage": "http://fomantic-ui.com",
  "author": "Jack Lukic <jacklukic@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fomantic/Fomantic-UI-Docs.git"
  },
  "scripts": {
    "serve": "node --stack-size=65500 /usr/local/bin/docpad run",
    "deploy": "node --stack-size=65500 /usr/local/bin/docpad  deploy-ghpages --env static"
  },
  "dependencies": {
    "docpad": "~6.79.4",
    "docpad-plugin-babel": "~2.2.1",
    "docpad-plugin-copy": "~2.1.3",
    "docpad-plugin-eco": "~2.3.0",
    "docpad-plugin-partials": "~2.12.0"
  },
  "devDependencies": {
    "docpad-plugin-ghpages": "~2.6.1"
  }
}
